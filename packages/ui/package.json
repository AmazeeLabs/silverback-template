{
  "name": "@custom/ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./intl": [
      "./build/utils/intl/index.js"
    ],
    "./routes/*": [
      "./build/components/Routes/*.js"
    ]
  },
  "typesVersions": {
    "*": {
      "routes/*": [
        "build/components/Routes/*.d.ts"
      ],
      "intl": [
        "build/utils/intl/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prep:types": "tsc --emitDeclarationOnly",
    "prep:scripts": "swc ./src -d ./build",
    "prep:styles": "NODE_ENV=production pnpm postcss tailwind.css -o build/styles.css",
    "prep:gutenberg": "NODE_ENV=production PREFIX=gutenberg pnpm postcss tailwind.css -o build/gutenberg.css",
    "prep:i18n": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --ignore='**/*.stories.ts*' --out-file build/translatables.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'\n",
    "build": "storybook build",
    "dev": "storybook dev -p 6006",
    "start": "serve storybook-static -p 6006 > /dev/null 2>&1",
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.eslintignore\"",
    "test:unit": "vitest run --passWithNoTests",
    "test:storybook": "test-storybook --coverage",
    "test:integration": "start-test start http://localhost:6006 test:storybook && pnpm report",
    "report": "nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook"
  },
  "dependencies": {
    "@amazeelabs/react-intl-rsc": "^1.1.9",
    "@custom/schema": "workspace:*",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.17",
    "clsx": "^1.2.1",
    "framer-motion": "^10.12.4",
    "hast-util-is-element": "^2.1.3",
    "hast-util-select": "^5.0.5",
    "unified": "^10.1.2",
    "framer-motion": "^10.12.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@amazeelabs/cloudinary-responsive-image": "^1.2.11",
    "@formatjs/cli": "^6.1.0",
    "@storybook/addon-actions": "^7.0.17",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.17",
    "@storybook/addon-interactions": "^7.0.17",
    "@storybook/addon-links": "^7.0.17",
    "@storybook/blocks": "^7.0.17",
    "@storybook/react": "^7.0.17",
    "@storybook/react-vite": "^7.0.17",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/jest": "^0.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.55",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/react": "^14.0.0",
    "@types/hast": "^2.3.4",
    "@types/react": "^18.2.0",
    "@types/react-body-classname": "^1.1.7",
    "@types/react-dom": "^18.2.1",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "chromatic": "^6.17.3",
    "cssnano": "^6.0.0",
    "eslint-plugin-formatjs": "^4.10.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "happy-dom": "^9.19.2",
    "nyc": "^15.1.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-prefix-selector": "^1.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.0",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.17",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-turbosnap": "^1.0.2",
    "vitest": "^0.31.1"
  }
}
