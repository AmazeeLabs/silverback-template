{
  "name": "@amazeelabs/token-auth-middleware",
  "description": "Middleware for token based authentication",
  "version": "1.0.0",
  "main": "build/src/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.gitignore\"",
    "test:unit": "vitest run --passWithNoTests",
    "prep": "rm -rf build && swc ./src/ -d ./build && tsc --emitDeclarationOnly",
    "build": "pnpm prep",
    "watch": "swc ./src/ -d ./build --watch"
  },
  "dependencies": {
    "jsrsasign": "^11.1.0"
  },
  "devDependencies": {
    "@types/jsrsasign": "^10.5.12",
    "@amazeelabs/eslint-config": "1.4.43",
    "@amazeelabs/prettier-config": "1.1.3",
    "@swc/cli": "0.3.9",
    "@swc/core": "1.4.2",
    "vitest": "^1.3.1"
  },
  "keywords": [],
