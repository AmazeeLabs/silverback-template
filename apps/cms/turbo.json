{
  "extends": ["//"],
  "pipeline": {
    "build": {
      "inputs": ["config/**"],
      "outputs": ["web/sites/default/files"]
    },
    "prep": {
      "inputs": ["composer.json", "composer.lock", "scaffold/**"],
      "outputs": ["web/**", "!web/sites/default/files", "vendor/**", "!**/.git"]
    },
    "start": {
      "dependsOn": ["build"]
    }
  }
}
