{
  "extends": ["//"],
  "pipeline": {
    "dev": {
      "persistent": true,
      "dependsOn": ["prep", "^prep"]
    },
    "prep": {
      "outputs": ["src/gatsby-fragments.js", "styles.css"]
    },
    "test:static": {
      "inputs": ["src/**", "!src/gatsby-fragments.js"]
    },
    "build": {
      "env": ["DRUPAL_INTERNAL_URL", "DRUPAL_EXTERNAL_URL"],
      "dependsOn": ["@custom/cms#prep", "prep", "^prep"],
      "inputs": [
        "netlify/**",
        "src/**",
        "static/**",
        "gatsby-browser.ts",
        "gatsby-config.mjs",
        "gatsby-node.mjs",
        "gatsby-ssr.ts",
        "image.mjs",
        "netlify.toml"
      ],
      "outputs": [
        ".cache/**",
        ".netlify/**",
        "public/**",
        ".env",
        "autoload.mjs",
        "styles.css"
      ]
    }
  }
}
