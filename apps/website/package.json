{
  "name": "@custom/website",
  "private": true,
  "dependencies": {
    "@amazeelabs/gatsby-source-silverback": "^1.10.2",
    "@amazeelabs/publisher": "^2.1.10",
    "@custom/schema": "workspace:*",
    "@custom/ui": "workspace:*",
    "gatsby": "^5.7.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "react": "experimental",
    "react-dom": "experimental"
  },
  "devDependencies": {
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11"
  },
  "optionalDependencies": {
    "netlify-cli": "^13.0.0",
    "start-server-and-test": "^2.0.0"
  },
  "scripts": {
    "test:static": "tsc --noEmit && eslint \"**/*.{ts,tsx,js,jsx}\" --ignore-path=\"./.eslintignore\"",
    "prep": "pnpm run prep:styles && pnpm run prep:fragments",
    "prep:styles": "rm -f styles.css && cp node_modules/@custom/ui/build/styles.css styles.css",
    "prep:fragments": "rm -rf src/gatsby-fragments.js && cp node_modules/@custom/schema/build/gatsby-fragments.js src/gatsby-fragments.js",
    "build:gatsby": "gatsby build",
    "start:cms": "pnpm run --filter @custom/cms start",
    "build": "start-test start:cms 8888 build:gatsby",
    "start": "publisher",
    "serve": "netlify dev --dir=public --port=8000",
    "dev:start": "rm -f styles.css && cp node_modules/@custom/ui/build/styles.css styles.css && gatsby develop --open",
    "dev": "start-test start:cms 8888 'pnpm turbo dev:start --output-logs=new-only'",
    "clean": "gatsby clean"
  }
}
