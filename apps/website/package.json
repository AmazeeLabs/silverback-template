{
  "name": "@custom/website",
  "private": true,
  "dependencies": {
    "@amazeelabs/bridge-gatsby": "^1.1.1",
    "@amazeelabs/cloudinary-responsive-image": "^1.2.11",
    "@amazeelabs/gatsby-silverback-cloudinary": "^1.0.18",
    "@amazeelabs/gatsby-source-silverback": "^1.10.6",
    "@amazeelabs/publisher": "^2.1.17",
    "@amazeelabs/strangler-netlify": "^1.1.0",
    "@custom/schema": "workspace:*",
    "@custom/ui": "workspace:*",
    "gatsby": "^5.9.0",
    "gatsby-plugin-manifest": "^5.9.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "netlify-cli": "^15.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "happy-dom": "^9.19.2",
    "start-server-and-test": "^2.0.0",
    "vitest": "^0.31.1"
  },
  "scripts": {
    "test:static": "tsc --noEmit && eslint '**/*.{ts,tsx,js,jsx}' --ignore-path='./.gitignore'",
    "prep": "pnpm run prep:styles && pnpm run prep:fragments && pnpm run prep:cloudinary",
    "prep:styles": "rm -f styles.css && cp node_modules/@custom/ui/build/styles.css styles.css",
    "prep:fragments": "rm -rf src/gatsby-fragments.js && cp node_modules/@custom/schema/build/gatsby-fragments.js src/gatsby-fragments.js",
    "prep:cloudinary": "rm -f static/mock-cloudinary-worker.js && rm -f static/mock-cloudinary-register.js && cp ./node_modules/@amazeelabs/cloudinary-responsive-image/worker/iife/* static/",
    "build:gatsby": "pnpm build:dotenv && gatsby build",
    "build:dotenv": "rm -rf .env && echo \"DRUPAL_EXTERNAL_URL='$DRUPAL_EXTERNAL_URL'\nDRUPAL_INTERNAL_URL='$DRUPAL_INTERNAL_URL'\" >> .env",
    "rebuild": "gatsby build",
    "start:cms": "pnpm run --filter @custom/cms start",
    "build": "start-test start:cms 8888 build:gatsby",
    "start": "publisher",
    "serve": "netlify dev --dir=public --port=8000",
    "dev": "publisher",
    "open": "open http://127.0.0.1:8000/___status/",
    "turbo:dev": "start-test start:cms 8888 'pnpm turbo dev --output-logs=new-only'",
    "clean": "gatsby clean"
  }
}
